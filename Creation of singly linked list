#include <iostream>
using namespace std;

class Node {
public:
    int data;
    Node* next;

    Node(int val) {
        data = val;
        next = nullptr;
    }
};

int main() {
    Node* head = new Node(0); // Dummy head node
    Node* temp = head;
    int size;

    cout << "Enter the number of nodes: ";
    cin >> size;

    cout << "Enter the values of the nodes:" << endl;
    for (int i = 0; i < size; i++) {
        int d;
        cin >> d;
        Node* t = new Node(d);
        temp->next = t;
        temp = temp->next;
    }

    // Print the linked list
    Node* ptr = head->next; // Skip the dummy head node
    while (ptr != nullptr) {
        cout << ptr->data << " ";
        ptr = ptr->next;
    }
}
